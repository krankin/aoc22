```elixir
defmodule Day6 do
  def run(input, amt) do
    0..String.length(input)
    |> Enum.find(fn x ->
      m =
        String.slice(input, x..(x + (amt - 1)))
        |> String.to_charlist()
        |> MapSet.new()

      MapSet.size(m) > amt - 1
    end)
    |> Kernel.+(amt)
  end
end

part1 = Day6.run(input, 4)
part2 = Day6.run(input, 14)
